{
  "expo": {
    "name": "EteSync Notes",
    "slug": "etesync_notes",
    "version": "1.6.0",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fdf6df"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.etesync.notes"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "themeColor": "#ffc107"
    },
    "sdkVersion": "39.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "androidStatusBar": {
        "translucent": true
    },
    "android": {
      "package": "com.etesync.notes"
    }
  }
}
